{
  "name": "jekyll-parser",
  "version": "0.0.1",
  "description": "Node.js library for parsing jekyll projects",
  "main": "lib/index.js",
  "watch": {
    "test": {
      "patterns": [
        "src",
        "features"
      ],
      "extensions": "js,feature"
    },
    "build": {
      "patterns": ["src"],
      "extensions": "js"
    }
  },
  "scripts": {
    "test": "babel-node node_modules/.bin/cucumber-js --format summary",
    "build": "babel src --out-dir lib",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unpress/jekyll-parser.git"
  },
  "keywords": [
    "jekyll",
    "parser"
  ],
  "author": "Niklas NÃ¤rhinen <niklas@narhinen.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unpress/jekyll-parser/issues"
  },
  "homepage": "https://github.com/unpress/jekyll-parser",
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "bluebird": "^3.3.3",
    "chai": "^3.5.0",
    "cucumber": "^0.9.5",
    "lodash.frompairs": "^4.0.1",
    "mkdirp": "^0.5.1",
    "npm-watch": "^0.1.1",
    "tmp": "0.0.28"
  },
  "dependencies": {
    "lodash.omit": "^4.1.0",
    "walk": "^2.3.9",
    "yaml-front-matter": "^3.4.0"
  }
}
